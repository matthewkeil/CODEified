{
  "name": "codeified",
  "version": "0.1.0",
  "license": "MIT",
  "author": "Matthew Keil",
  "description": "Blockchain Based Decentralized Democratic Governance Through Transparent and Automated Aggregation of Individually CODEified Constitutions",
  "scripts": {
    "ts-node": "ts-node",
    "build:ts": "tsc --project tsconfig.build.json",
    "build:providers": "webpack",
    "build:layer": "ts-node bin/buildLayer",
    "build:frontend": "npm run generate:frontend && npm run move:frontend",
    "frontend:generate": "cd frontend && npm run generate",
    "frontend:move": "cross-env-shell \"mv frontend/dist dist/frontend\n",
    "build": "npm run build:ts && npm run build:providers && npm run build:layer",
    "synth": "cross-env-shell \"node_modules/.bin/cdk synth\"",
    "deploy": "cross-env-shell \"ts-node node_modules/.bin/cdk deploy '*'\""
  },
  "devDependencies": {
    "@aws-cdk/aws-apigateway": "1.101.0",
    "@aws-cdk/aws-certificatemanager": "1.101.0",
    "@aws-cdk/aws-cloudfront": "1.101.0",
    "@aws-cdk/aws-cloudfront-origins": "1.101.0",
    "@aws-cdk/aws-cognito": "1.101.0",
    "@aws-cdk/aws-dynamodb": "1.101.0",
    "@aws-cdk/aws-lambda": "1.101.0",
    "@aws-cdk/aws-lambda-event-sources": "1.101.0",
    "@aws-cdk/aws-logs": "1.101.0",
    "@aws-cdk/aws-route53": "1.101.0",
    "@aws-cdk/aws-route53-targets": "1.101.0",
    "@aws-cdk/aws-s3": "1.101.0",
    "@aws-cdk/aws-s3-deployment": "1.101.0",
    "@aws-cdk/core": "1.101.0",
    "@types/jest": "^24.0.19",
    "@types/node": "^15.0.1",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "aws-cdk": "1.101.0",
    "aws-sdk": "^2.773.0",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "terser-webpack-plugin": "^5.1.1",
    "ts-loader": "^9.1.2",
    "ts-node": "^9.0.0",
    "typescript": "~3.9.3",
    "webpack": "^5.36.2",
    "webpack-cli": "^4.7.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "git add"
    ]
  },
  "dependencies": {
    "custom-resource-provider": "0.0.5"
  }
}
